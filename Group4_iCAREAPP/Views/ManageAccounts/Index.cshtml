@model IEnumerable<Group4_iCAREAPP.Models.UserPassword>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.userName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.encryptedPassword)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.passwordExpiryTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.userAccountExpiryDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.iCareUser.name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.userName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.encryptedPassword)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.passwordExpiryTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.userAccountExpiryDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.iCareUser.name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
