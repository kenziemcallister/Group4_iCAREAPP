@model Group4_iCAREAPP.Models.iCareWorker

@{
    ViewBag.Title = "Profile";
}

<h2>Profile</h2>

<div class="profile-details">
    <table class="table">
        <tr>
            <th>ID</th>
            <td>@Model.ID</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>@ViewBag.UserName</td>
        </tr>
        <tr>
            <th>Profession</th>
            <td>@Model.profession</td>
        </tr>
        <tr>
            <th>Creator</th>
            <td>@Model.creator</td>
        </tr>
        <tr>
            <th>Geographical Unit</th>
            <td>@Model.GeoCodes.description</td>
        </tr>
    </table>
</div>

<p>
   <button type="button" onclick="location.href='@Url.Action("Index")'">Back</button>
</p>

